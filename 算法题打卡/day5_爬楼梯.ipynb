   "source": [
    "import math\n",
    "def fibonacci(n):\n",
    "    sqrt5 = math.sqrt(5)\n",
    "    phi = (1 + sqrt5) / 2\n",
    "    psi = (1 - sqrt5) / 2\n",
    "    return (phi**n - psi**n) / sqrt5\n",
    "\n",
    "n=int(input())\n",
    "print(round(fibonacci(n+1)))"
   ]
  },
